			Title = "error",
			Text = "Player not found or player died!",
			Duration = 6
		})
	end
end)

Section:NewTextBox("ban a player", "attempt to ban user", function(txt)
	local targetName = findPlayer(txt)

	if targetName ~= nil then
		targetName = findPlayer(txt).Name
		local s,e = pcall(function()
			local args = {
				[1] = ";crash "..targetName
			}


			game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
			print("oops")
			repeat wait(0.2)
			until game.Players:WaitForChild(targetName,0.1) == nil

			wait(1)
			local args = {
				[1] = ";pban "..targetName.." Abuse",
				[2] = {
					["length"] = "Infinite",
					["lengthTime"] = "0m0h0d",
					["server"] = "All"
				}
			}

			game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))

			wait(1)
			local args = {
				[1] = ";directBan "..targetName.." Abuse",
				[2] = {
					["length"] = "Infinite",
					["lengthTime"] = "0m0h0d",
					["server"] = "All"
				}
			}
		end)
		if e then
			game.StarterGui:SetCore("SendNotification",{
				Title = "error",
				Text = e,
				Duration = 6
			})

		end
	else
		game.StarterGui:SetCore("SendNotification",{
			Title = "error",
			Text = "Player not found or player died!",
			Duration = 6
		})
	end
end)

local autocrash = {}

Section:NewTextBox("crashban a player", "puts a player in a auto crash list", function(txt)
	local targetName = findPlayer(txt)

	if targetName ~= nil then
		targetName = findPlayer(txt).Name
		local s,e = pcall(function()
			table.insert(autocrash,targetName)
			local args = {
				[1] = ";crash "..targetName,
			}

			game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
		end)
		if e then
			game.StarterGui:SetCore("SendNotification",{
				Title = "error",
				Text = e,
				Duration = 6
			})

		end
	else
		game.StarterGui:SetCore("SendNotification",{
			Title = "error",
			Text = "Player not found or player died!",
			Duration = 6
		})
	end
end)

game.Players.PlayerAdded:Connect(function(plr)
	for _,v in pairs(autocrash) do
		if v == plr.Name then
			local args = {
				[1] = ";crash "..plr.Name,
			}

			game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
			game.StarterGui:SetCore("SendNotification",{
				Title = "Crash ban",
				Text = "crashed a crash banned player"
			})
		end
	end
end)

