Section:NewTextBox("banish a player", "banish user", function(txt)
	local target = findPlayer(txt)

	if target ~= nil then
		local s,e = pcall(function()
			local args = {
				[1] = ";apparate "..findPlayer(txt).Name.." 5680667182"
			}

			game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
		end)
		if e then
			game.StarterGui:SetCore("SendNotification",{
				Title = "error",
				Text = e,
				Duration = 6
			})

		end
	else
		game.StarterGui:SetCore("SendNotification",{
			Title = "error",
			Text = "Player not found or player died!",
			Duration = 6
		})
	end
end)


