local Tab = Window:MakeTab({
	Name = "Admin stuff",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "OP"
})

Tab:AddButton({
	Name = "Admin UI",
	Callback = function()
      			    local player = game.Players.LocalPlayer
    local adminUI = player.PlayerGui:FindFirstChild("AdminUI")

    if adminUI then
        adminUI.Enabled = true
        
        local isAdminValue = adminUI:FindFirstChild("IsAdmin")
        if isAdminValue and isAdminValue:IsA("BoolValue") then
            isAdminValue.Value = true
        else
            warn("IsAdmin BoolValue not found in AdminUI")
        end
        
        local button = adminUI:FindFirstChild("Button")
        if button then
            button.Visible = true
        else
            warn("Button not found in AdminUI")
        end
    else
        warn("AdminUI not found")
    end
  	end    
})

Tab:AddButton({
	Name = "God's hand ability",
	Callback = function()
	local args = {}
      	game:GetService("ReplicatedStorage").TimeStop:FireServer(unpack(args))
  	end    
})

Tab:AddButton({
	Name = "Spawn jet orb",
	Callback = function()
	local args = {}
      	game:GetService("ReplicatedStorage").SpawnOrb:FireServer(unpack(args))
  	end    
})

Tab:AddButton({
	Name = "God mode",
	Callback = function()
	local args = {}
      	game:GetService("ReplicatedStorage").Invincible:FireServer(unpack(args))
  	end    
})

