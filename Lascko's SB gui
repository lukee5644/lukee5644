Tab:AddButton({
	Name = "Train Ability",
	Callback = function()
		local plr = game:GetService("Players").LocalPlayer
		local chr = plr.Character or plr.CharacterAdded:Wait()
		local event = chr.train.Event
		local args = {}
		event:FireServer(unpack(args))
  	end    
})

