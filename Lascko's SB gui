local uilibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kiet1308/tvkhub/main/rac"))()
local windowz = uilibrary:CreateWindow("OP", "Lacko's slap battles", true)

local Page1 = windowz:CreatePage("Modifiers")


local Section1 = Page1:CreateSection("WS/JP and more")

Section1:CreateSlider("Walkspeed", {Min = 16, Max = 500, DefaultValue = 30}, function(Value)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
end)

Section1:CreateSlider("JumpPower", {Min = 50, Max = 500, DefaultValue = 30}, function(Value)
   game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
end)

Section1:CreateButton("Anti-Void", function ()
   local voidpart = Instance.new("Part")
   voidpart.Parent = workspace
   voidpart.Position = Vector3.new(-1503.54431, 1701.54309, -377.699615, 0, 1, -0, -1, 0, 0, 0, 0, 1)
   voidpart.Size = Vector3.new(2048, 1, 2048)
   voidpart.Transparency = 1
   voidpart.Name = "anti-void"
   voidpart.Anchored = true
end)

local Page1 = windowz:CreatePage("Killstreak")
local Section1 = Page1:CreateSection("Ignore errors on console")

Section1:CreateButton("+1 kill", function ()
	local args = {}
	game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
end)

Section1:CreateButton("10 kills", function ()
	    local totalKills = 10
    local killsToGive = 1

    for i = 1, totalKills do
        local args = {
            killsToGive
        }
        game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
        wait(0.01)
    end
end)

