local colors = {
    SchemeColor = Color3.fromRGB(87, 88, 87),
    Background = Color3.fromRGB(0, 0, 0),
    Header = Color3.fromRGB(15, 15, 15),
    TextColor = Color3.fromRGB(255, 255, 255),
    ElementColor = Color3.fromRGB(10, 10, 10)
}
print("executed gui enjoy!")

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Lackos slap battle GUI", colors)

-- Modifiers
local Tab = Window:NewTab("Modifiers")
local Section = Tab:NewSection("Walkspeed and Jumppower")

Section:NewSlider("Speed", "Changes your walkspeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("JumpPower", "Changes your jumppower", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s -- Fixed property name
end)

Section:NewButton("TP To Lobby", "Teleports you to the lobby", function()
    local player = game.Players.LocalPlayer
    local teleportPart = game.Workspace:FindFirstChild("Lobby"):FindFirstChild("SpawnLocation") -- Ensure proper part path

    local function teleportPlayer()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = teleportPart.CFrame + Vector3.new(0, 5, 0) -- Teleport above the part
        end
    end

    teleportPlayer()
end)

-- OP Stuff --
local Tab = Window:NewTab("OP Stuff")
local Section = Tab:NewSection("Killstreak")
local Section = Tab:NewSection("ignore errors on console")

Section:NewButton("10 Kills", "yes", function()
    local totalKills = 10
    local killsToGive = 1

    for i = 1, totalKills do
        local args = {
            killsToGive
        }
        game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
        wait(0)
    end
end)

