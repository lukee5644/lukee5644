print("executed gui")
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Lacko's slap battle gui", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
	Name = "Notification!",
	Content = "Thanks for using my script :D",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Modifiers",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "WS/JP and more"
})
Tab:AddButton({
	Name = "Anti-Void (PERM)",
	Callback = function()
      	 local voidpart = Instance.new("Part")
   		voidpart.Parent = workspace
   		voidpart.Position = Vector3.new(-1503.54431, 1701.54309, -377.699615, 0, 1, -0, -1, 0, 0, 0, 0, 1)
  		 voidpart.Size = Vector3.new(2048, 1, 2048)
  		 voidpart.Transparency = 1
  		 voidpart.Name = "anti-void"
  		 voidpart.Anchored = true
  	end    
})

Tab:AddButton({
    Name = "TP To lobby",
    Callback = function()
        local plr = game.Players.LocalPlayer
        local teleportPart = game.Workspace:FindFirstChild("Lobby"):FindFirstChild("SpawnLocation")
        
        if teleportPart then
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                plr.Character.HumanoidRootPart.CFrame = teleportPart.CFrame + Vector3.new(0, 5, 0)
            else
                warn("Player's character or HumanoidRootPart is not available")
            end
        else
            warn("Teleport part not found in Workspace")
        end
    end
})

Tab:AddSlider({
	Name = "Speed",
	Min = 16,
	Max = 5000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tab:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 5000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "JumpPower",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

local Tab = Window:MakeTab({
	Name = "Killstreak",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Get kills"
})

Tab:AddButton({
	Name = "Get a kill",
	Callback = function()
      		local args = {}
			  game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
  	end    
})

Tab:AddButton({
	Name = "Get 10 kills",
	Callback = function()
			    local totalKills = 10
    local killsToGive = 1

    for i = 1, totalKills do
        local args = {
            killsToGive
        }
        game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
        wait(0.01)
  	end    
end
})

