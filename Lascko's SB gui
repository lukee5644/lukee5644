    game:GetService("ReplicatedStorage").givekill:FireServer(unpack(args))
end)

local Section = Tab:NewSection("Admin stuff")

Section:NewButton("Admin UI", "works on yourself only except for ban", function()
    local player = game.Players.LocalPlayer
    local adminUI = player.PlayerGui:FindFirstChild("AdminUI")

    if adminUI then
        adminUI.Enabled = true
        
        local isAdminValue = adminUI:FindFirstChild("IsAdmin")
        if isAdminValue and isAdminValue:IsA("BoolValue") then
            isAdminValue.Value = true
        else
            warn("IsAdmin BoolValue not found in AdminUI")
        end
        
        local button = adminUI:FindFirstChild("Button")
        if button then
            button.Visible = true
        else
            warn("Button not found in AdminUI")
        end
    else
        warn("AdminUI not found")
    end
end)

Section:NewButton("TimeStop", "obvious", function()
local args = {}

game:GetService("ReplicatedStorage").TimeStop:FireServer(unpack(args))
wait(8)
local player = game.Players.LocalPlayer
local chr = player.Character
chr.HumanoidRootPart.Anchored = false
chr:FindFirstChild("Left Arm").Anchored = false
chr:FindFirstChild("Right Arm").Anchored = false
chr:FindFirstChild("Left Leg").Anchored = false
chr:FindFirstChild("Right Leg").Anchored = false
chr:FindFirstChild("Head").Anchored = false
chr:FindFirstChild("LA").Anchored = false
chr:FindFirstChild("LL").Anchored = false
chr:FindFirstChild("RA").Anchored = false
chr:FindFirstChild("RL").Anchored = false
chr:FindFirstChild("Torso").Anchored = false
end)

Section:NewButton("Spawn Flight orb", "spawns an orb", function()
local args = {}

game:GetService("ReplicatedStorage").SpawnOrb:FireServer(unpack(args))
end)

Section:NewButton("God mode", "invincibility", function()
local args = {}

game:GetService("ReplicatedStorage").Invincible:FireServer(unpack(args))
end)

Section:NewButton("Unanchor yourself (Use against time stop)", "obvious", function()
	local player = game.Players.LocalPlayer
local chr = player.Character
chr.HumanoidRootPart.Anchored = false
chr:FindFirstChild("Left Arm").Anchored = false
chr:FindFirstChild("Right Arm").Anchored = false
chr:FindFirstChild("Left Leg").Anchored = false
chr:FindFirstChild("Right Leg").Anchored = false
chr:FindFirstChild("Head").Anchored = false
chr:FindFirstChild("LA").Anchored = false
chr:FindFirstChild("LL").Anchored = false
chr:FindFirstChild("RA").Anchored = false
chr:FindFirstChild("RL").Anchored = false
chr:FindFirstChild("Torso").Anchored = false
end)

